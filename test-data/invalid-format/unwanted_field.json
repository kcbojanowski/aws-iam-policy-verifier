{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Stmt1464034295000",
      "Effect": "Allow",
      "Password": "confidential",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:DeleteObject",
        "s3:ListMultipartUploadParts",
        "s3:PutObject",
        "s3:GetObject"
      ],
      "Resource": ["arn:aws:s3:::my_bucket/*"]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads"
      ],
      "Resource": ["arn:aws:s3:::my_bucket"]
    }
  ]
}
